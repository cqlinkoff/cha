{
  "env": {
    "commonjs": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "browsers": "ie >= 9"
          }
        }],
        "@babel/preset-react"
      ]
    },
    "es": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "browsers": "ie >= 9"
          },
          "modules": false,
          "loose": true
        }],
        "@babel/preset-react"
      ]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ]
}
