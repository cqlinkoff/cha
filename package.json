{
  "name": "@cqmbr/cha",
  "version": "1.0.0",
  "description": "A Set of React Components",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "lint": "npm run lint:css",
    "lint:js": "eslint ./src/** --ext .js --ext .jsx",
    "lint:css": "stylelint ./src/**/*.less",
    "prebuild": "npm run lint",
    "build": "npm run build:es && npm run build:commonjs",
    "build:commonjs": "BABEL_ENV=commonjs babel src -d lib",
    "build:es": "BABEL_ENV=es babel src -d es"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cqmbr/cha.git"
  },
  "keywords": [
    "react",
    "components",
    "component",
    "react-component",
    "ui",
    "ui-kit"
  ],
  "author": "kagawagao <kingsongao1221@gmail.com> (https://kingsongao.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cqmbr/cha/issues"
  },
  "homepage": "https://github.com/cqmbr/cha#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0-beta.54",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.4.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}
