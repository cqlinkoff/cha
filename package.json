{
  "name": "@cqlink/cha",
  "version": "1.0.0",
  "description": "A Set of React Components",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:js": "eslint ./src/** --ext .js --ext .jsx",
    "lint:css": "stylelint ./src/**/*.less",
    "prebuild": "npm run lint",
    "build": "npm run build:es && npm run build:commonjs",
    "build:commonjs": "BABEL_ENV=commonjs babel src -d lib",
    "build:es": "BABEL_ENV=es babel src -d es",
    "precommit": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cqlink/cha.git"
  },
  "keywords": [
    "react",
    "components",
    "component",
    "react-component",
    "ui",
    "ui-kit"
  ],
  "author": "kagawagao <kingsongao1221@gmail.com> (https://kingsongao.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cqlink/cha/issues"
  },
  "homepage": "https://github.com/cqlink/cha#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "classnames": "^2.2.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
